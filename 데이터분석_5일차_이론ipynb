        "import numpy as np\n",
        "\n",
        "def sigmoid(x):\n",
        "  return 1/ (1+np.exp(-x))\n",
        "\n",
        "x_range = np.arange(-10, 10.1, 0.1)\n",
        "y_range = [sigmoid(x) for x in x_range]\n",
        "\n",
        "plt.plot(x_range, y_range)"
